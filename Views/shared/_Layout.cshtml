<!DOCTYPE html> 
<html>
    <head>
        <meta name="viewport" content="width=device-width"/>
        <title>@ViewBag.Title</title>
    </head>
    <style>
        ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #333;
}

li {
  float: left;
}

li a {
  display: block;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

li a:hover {
  background-color: #111;
}
    </style>
    <body>
        <nav>
            <ul>
                <li>
                    <a asp-controller="home" asp-action="index">List</a>
                </li>
                 <li>
                    <a asp-controller="home" asp-action="Create">Create</a>
                </li>
            </ul>
        </nav>
        <div>
            @RenderBody()
        </div>
    </body>
</html>